{
  "name": "smart-stock-hub-api",
  "version": "0.0.1",
  "description": "Monitor product stock in real time, avoiding stockouts, excess stock, or losses due to expiration. Record entries, exits, adjustments, and transfers between warehouses.",
  "author": "Luis Ramon Diaz <diazluisramon.dev@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs --dataSource ./dist/database/data-source.js",
    "migration:gen": "npm run typeorm migration:generate ./src/database/migrations/$npm_config_name",
    "migration:run": "npm run typeorm migration:run",
    "migration:show": "npm run typeorm migration:show",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:drop": "npm run typeorm schema:drop",
    "seed:dev": "ts-node src/database/seed.ts"
  },
